      "axios": "^0.18.0",
      "babel-plugin-import": "^1.8.0",
      "enzyme": "^3.7.0",
      "enzyme-adapter-react-16": "^1.7.0",
      "enzyme-to-json": "^3.3.4",
      "eslint-loader": "^2.1.1",
      "jsonwebtoken": "^8.3.0",
      "node-sass": "^4.9.3",
      "prop-types": "^15.6.2",
      "react": "^16.6.1",
      "react-app-rewire-less": "^2.1.3",
      "react-app-rewired": "^1.6.2",
      "react-dev-utils": "^6.1.1",
      "react-dom": "^16.6.1",
      "react-redux": "^5.1.1",
      "react-router-dom": "^4.3.1",
      "react-scripts": "^2.1.1",
      "react-x-editable": "^0.0.10-beta",
      "redux": "^4.0.1",
