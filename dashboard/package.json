    },
    "pre-commit": [
      ""
    ],
    "jest": {
      "verbose": false,
      "collectCoverage": true,
      "notify": true,
      "collectCoverageFrom": [
        "!**/node_modules/**",
        "!**/vendor/**"
      ]
    },
    "proxy": "http://localhost:5000",
    "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
